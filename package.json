{
  "name": "vitest-extended",
  "version": "0.0.1",
  "description": "Additional Vitest matchers",
  "type": "module",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types/index.d.ts"
  ],
  "exports": {
    "./all": {
      "default": "./dist/all.js",
      "node": "./dist/all.cjs",
      "types": "./types/index.d.ts"
    },
    ".": {
      "default": "./dist/index.js",
      "node": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rimraf ./dist/* && tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "precommit": "lint-staged",
    "test": "yarn test:base --run",
    "test:base": "vitest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test:base --watch",
    "typecheck": "tsc --noEmit",
    "dev:docs": "cd website && yarn start"
  },
  "keywords": [
    "vitest",
    "matchers",
    "extend",
    "extended",
    "test",
    "testing",
    "assertions"
  ],
  "author": "Tanner B. Hess Webber <tanner@hesswebber.com>",
  "license": "MIT",
  "repository": "tbhesswebber/vitest-extended",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@jest/expect-utils": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitest/coverage-v8": "^0.34.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest-matcher-utils": "^29.6.2",
    "jest-serializer-ansi-escapes": "^2.0.1",
    "jest-watch-typeahead": "^2.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "jest-diff": "^29.0.0",
    "jest-get-type": "^29.0.0"
  },
  "engines": {
    "node": "^16.10.0 || >=18.0.0"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.md": "prettier --write"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "vitest": ">=0.32.0"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": false
    }
  }
}
